<Header wx: title="{{title}}" url="back" className="{{className}}" />

<block wx:if="{{type=='list'}}">
  <List type="base" data="{{data}}" />
  <List type="detail" data="{{data}}" />
  <List type="msg" data="{{data}}" />
</block>

<block wx:if="{{type=='grid'}}">
  <Grid type="base" columns="5" border="true" />
  <Grid type="separate" />
</block>