.tab_bar {
  display: flex;
  background: #fff;
  border-top: 1rpx #eee solid;
  box-sizing: border-content;
  height: 100rpx;
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}

.tab_bar .item {
  flex: 1;
  text-align: center;
  color: #888;
  padding: 10rpx 0;
}

.item.active {
  color: #33d1bf;
}

.item .title {
  font-size: 20rpx;
  line-height: 30rpx;
}

.item .iconfont {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 40rpx;
}
